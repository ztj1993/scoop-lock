    "homepage": "https://www.jetbrains.com/pycharm/",
    "license": {
        "identifier": "Proprietary",
        "url": "https://www.jetbrains.com/store/license.html"
    },
    "changelog": "https://www.jetbrains.com/pycharm/whatsnew/",
    "url": [
        "https://download.jetbrains.com/python/pycharm-professional-2021.2.2.exe#/cosi.7z"
    ],
    "hash": [
        "6e83cd869936728435c164d62b5fe7685665bc1dfdf62a3d816e79dd6c32c96e"
    ],
    "extract_to": "IDE",
    "installer": {
        "script": [
            "$bucket_dir = Find-BucketDirectory -Root -Name $bucket",
            "$script_bin = \"$bucket_dir\\scripts\\jetbrains\\portable.ps1\"",
            "& \"$script_bin\" $dir $persist_dir"
        ]
    },
    "architecture": {
        "64bit": {
            "bin": [
                "IDE\\bin\\pycharm64.exe",
                [
                    "IDE\\bin\\pycharm64.exe",
                    "pycharm"
                ]
            ],
            "shortcuts": [
                [
                    "IDE\\bin\\pycharm64.exe",
                    "JetBrains\\PyCharm Professional"
                ]
            ]
        },
        "32bit": {
            "bin": "IDE\\bin\\pycharm.exe",
            "shortcuts": [
                [
                    "IDE\\bin\\pycharm.exe",
                    "JetBrains\\PyCharm Professional"
                ]
            ]
        }
    },
    "persist": [
        "IDE\\bin\\idea.properties",
        "profile"
    ],
    "checkver": {
        "url": "https://data.services.jetbrains.com/products/releases?code=PCP&latest=true&platform=zip&type=release",
        "regex": "version\":\"(?<ver>[\\d.]+)\".*\"build\":\"(?<build>[\\d.]+)\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "url": "https://download.jetbrains.com/python/pycharm-professional-$matchHead.exe#/cosi.7z",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
